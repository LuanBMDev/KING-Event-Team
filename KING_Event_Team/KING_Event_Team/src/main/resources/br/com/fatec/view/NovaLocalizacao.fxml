            <String fx:value="botaoNormal" />
            <String fx:value="botaoRedondo" />
         </styleClass>
      </Button>
      <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="250.0" layoutY="300.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/LogotipoPNG.png" />
         </image>
      </ImageView>
      <Label fx:id="lblTitulo" alignment="CENTER" contentDisplay="CENTER" layoutX="128.0" prefHeight="75.0" prefWidth="344.0" styleClass="titulo" stylesheets="@../css/estilogeral.css" text="NOVA LOCALIZAÇÃO" textAlignment="CENTER" textFill="WHITE" underline="true">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtNomeLocal" layoutX="27.0" layoutY="116.0" prefHeight="25.0" prefWidth="547.0" />
      <Label layoutX="27.0" layoutY="99.0" text="Nome do Local:" textFill="WHITE" />
      <TextField fx:id="txtCEP" layoutX="27.0" layoutY="165.0" prefHeight="25.0" prefWidth="146.0" />
      <Label layoutX="27.0" layoutY="148.0" text="CEP:" textFill="WHITE" />
      <TextField fx:id="txtEndereco" layoutX="186.0" layoutY="165.0" prefHeight="25.0" prefWidth="268.0" />
      <Label layoutX="186.0" layoutY="148.0" text="Endereço:" textFill="WHITE" />
      <TextField fx:id="txtNumero" layoutX="466.0" layoutY="165.0" prefHeight="25.0" prefWidth="108.0" />
      <Label layoutX="466.0" layoutY="148.0" text="Número:" textFill="WHITE" />
      <TextField fx:id="txtCidade" layoutX="27.0" layoutY="217.0" prefHeight="25.0" prefWidth="344.0" />
      <Label layoutX="27.0" layoutY="200.0" text="Cidade:" textFill="WHITE" />
      <Label layoutX="384.0" layoutY="200.0" text="Tipo Local:" textFill="WHITE" />
      <ComboBox fx:id="cmbTipo" layoutX="384.0" layoutY="217.0" prefHeight="25.0" prefWidth="189.0" />
